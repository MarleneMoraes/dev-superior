package com.devsuperior.javacompleto.aula028;

public class Main {

	public static void main(String[] args) {
		double x = 3.0;
		double y = 4.0;
		double z = -5.0;
		double A, B, C;
		
		//RAIZ QUADRADA
		A = Math.sqrt(x);
		B = Math.sqrt(y);
		C = Math.sqrt(z);
		
		System.out.printf("Raiz quadrada de %.1f = %f", x, A);
		System.out.printf("Raiz quadrada de %.1f = %f", y, B);
		System.out.printf("Raiz quadrada de %.1f = %f", z, C);
	}

}
